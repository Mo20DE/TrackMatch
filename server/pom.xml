        </dependency>
        <dependency>
          <groupId>com.github.psambit9791</groupId>
          <artifactId>jdsp</artifactId>
          <version>3.1.0</version>
      </dependency>
    <dependency>
        <groupId>be.tarsos.dsp</groupId>
