        <version>2.5</version>
    </dependency>
    <dependency>
        <groupId>be.tarsos.dsp</groupId>
        <artifactId>jvm</artifactId>
        <version>2.5</version>
    </dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

